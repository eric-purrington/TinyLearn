<nav class="navbar bg-light">
    <h1>Eat Da Burger</h1>
</nav>
<center><img src="https://media.giphy.com/media/WBTUlyKBVlTTa/giphy.gif" /></center>
<div class="container my-5">
    <div class="row">
        <div class="col">
            <div class="card">
                <div class="card-header text-center">
                    <h3>Eaten</h3>
                </div>
                <div class="card-body">

                    {{#each DATA}}
                    <div>{{DATA}}</div>
                    {{/each }}

                </div>
            </div>
        </div>
    </div>
    <div class="row mt-5">
        <div class="col-md-6 offset-md-3">
            <div class="input-group mb-3">
                <input type="text" class="form-control" placeholder="Add a subject..." id="subjectName">
                <div class="input-group-append">
                    <button class="btn btn-outline-success" type="button" id="addSubject">Add it!</button>
                </div>
            </div>
        </div>
    </div>
</div>